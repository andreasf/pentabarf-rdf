  <rdf:Description rdf:about="http://fahrplan.u0d.de/Chaos_Communication_Camp_2011/event/4399">
    <rdf:type rdf:resource="http://fahrplan.u0d.de/schedule.owl#Lecture"/>
    <_4:hasDescription>At the end general security issues are explained.</_4:hasDescription>
    <_4:hasRoom rdf:resource="http://fahrplan.u0d.de/Chaos_Communication_Camp_2011/room/Baikonur"/>
    <_4:hasSpeaker rdf:resource="http://fahrplan.u0d.de/Chaos_Communication_Camp_2011/person/3271"/>
