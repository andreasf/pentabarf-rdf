    <_4:hasDescription>Talk image is courtesy of Greenpeace.</_4:hasDescription>
    <rdfs:seeAlso rdf:resource="http://www.stephalarcon.org/capstone/alarcon-poster3.pdf"/>
    <rdfs:seeAlso rdf:resource="http://www.scribd.com/doc/34887786/Electronic-Waste-What-s-Here-and-What-s-Next-by-Stephanie-Alarcon"/>
    <rdfs:seeAlso rdf:resource="http://www.stephalarcon.org/capstone/capstone-alarcon.pdf"/>
    <_4:hasRoom rdf:resource="http://fahrplan.u0d.de/Chaos_Communication_Camp_2011/room/Kourou"/>
